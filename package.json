{
  "name": "nextjs-sample",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "format": "prettier --write 'pages/**/*.{ts,tsx}'",
    "dev": "next dev",
    "build": "yarn format && next build",
    "export": "next export",
    "start": "yarn build && next start",
    "dev:server": "yarn build && cross-env NODE_ENV=development ts-node index.ts",
    "prod:server": "yarn build && cross-env NODE_ENV=production ts-node index.ts",
    "lint": "next lint"
  },
  "dependencies": {
    "body-parser": "^1.19.2",
    "cross-env": "^7.0.3",
    "express": "^4.17.3",
    "express-session": "^1.17.2",
    "log4js": "^6.4.2",
    "next": "12.1.0",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/express-session": "^1.17.4",
    "@types/node": "^17.0.21",
    "@types/react": "17.0.40",
    "autoprefixer": "^10.4.2",
    "eslint": "8.11.0",
    "eslint-config-next": "12.1.0",
    "postcss": "^8.4.8",
    "prettier": "2.5.1",
    "tailwindcss": "^3.0.23",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.2"
  }
}
